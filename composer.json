    "keywords": ["latex", "flarum", "flagrow", "math", "expressions", "tex"],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Pompili",
            "email": "matpompili@gmail.com"
        },
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel+flarum@klabbers.email",
            "homepage": "http://hyn.io"
        }
    ],
    "support": {
